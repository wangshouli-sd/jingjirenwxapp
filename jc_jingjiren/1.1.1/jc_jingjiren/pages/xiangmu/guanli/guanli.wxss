/* 洽谈中列表 */
.w-state1-item{
  padding: 20rpx;
  margin-top: 30rpx;
  border-bottom: 1rpx solid #EEEEEE; 
}
.w-state1-item-head{
 
}
.state1-item-title{
  font-size: 30rpx;
  color: #171717;
  position: relative;
}
.state1-item-title-line{
  width: 50rpx;
  height: 6rpx;
  background:#52A1FD;
  margin-top: 6rpx;
  margin-left: 4rpx;
}
.state1-item-time{
  font-size: 16rpx;
  color:#939292;
}
