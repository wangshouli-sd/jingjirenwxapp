}
.state1-progress{
  min-height: 100rpx;
}
/* .progress-top-line{
  width: 100%;
  height: 2rpx;
  background: #E9844E;
} */
.progress-step-item{
  position: relative;
}
.progress-step-line{
  position: absolute;
  width: 50%;
  height: 4rpx;
  background: #DDDDDD;
  top: 16rpx;
}
.progress-step-line-l{left: 0rpx;}
.progress-step-line-r{right: 0rpx;}

.progress-bottom-icon{
  width: 36rpx;height: 36rpx;
  border: 4rpx solid #DDDDDD;
  border-radius: 50%;
  margin: auto;
  position: relative;
  z-index: 2;
}
.progress-bottom-icon view{
  background: #DDDDDD;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  border: 5rpx solid white;
}
.progress-bottom-icon-more{
  width: 40rpx;
  height: 40rpx;
  background: #DDDDDD;
  border-radius: 50%;
  margin: auto;
  padding: 5rpx;
  color: white;
  font-size: 30rpx;
  line-height: 20rpx;
  text-align: center;
  font-weight: bolder;
}
.progress-bottom-title{
  margin-top: 10rpx;
  text-align: center;
}
.state1-progress-bottom .progress-step-item:nth-child(1) .progress-step-line-l,
.state1-progress-bottom .progress-step-item:last-child .progress-step-line-r{
  display: none;
}

.state1-step-complete .progress-step-line-l,
.state1-step-complete .progress-step-line-r{
  background: #E9844E;
}
.state1-step-complete .progress-bottom-icon{
  border: 4rpx solid #E9844E;
}
.state1-step-complete .progress-bottom-icon view{
  background: #E9844E;
}
.w-state1-item-foot{
  margin-top: 20rpx;
}
.w-state1-item-foot button{
  /* width:100rpx; */
  height:40rpx;
  border:1px solid rgba(181,181,181,1);
  border-radius:20rpx;
  display: inline-block;
  font-size: 24rpx;
  color: #101010;
  padding: 0rpx 30rpx;
  background: white;
}
